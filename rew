#!name = Youtube
#!desc = 去除广告

^https:\/\/rr[\w-]+\.googlevideo\.com\/initplayback\? url reject-dict

#^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/(browse|next|player|search|reel\/reel_watch_sequence|guide|account\/get_setting|get_watch) url script-response-body https://raw.githubusercontent.com/kergb/QuantumultX/refs/heads/main/youtube.js

//  信息流
^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/browse url script-response-body https://raw.githubusercontent.com/kergb/QuantumultX/refs/heads/main/youtube.js
//  搜索页结果
^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/search url script-response-body https://raw.githubusercontent.com/kergb/QuantumultX/refs/heads/main/youtube.js
//  指导
^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/guide url script-response-body https://raw.githubusercontent.com/kergb/QuantumultX/refs/heads/main/youtube.js
//  获取配置
^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/account\/get_setting url script-response-body https://raw.githubusercontent.com/kergb/QuantumultX/refs/heads/main/youtube.js
//  播放页
^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/get_watch url script-response-body https://raw.githubusercontent.com/kergb/QuantumultX/refs/heads/main/youtube.js
^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/next url script-response-body https://raw.githubusercontent.com/kergb/QuantumultX/refs/heads/main/youtube.js
//  短视频
^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/player\? url script-response-body https://raw.githubusercontent.com/kergb/QuantumultX/refs/heads/main/youtube.js
^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/reel\/reel_watch_sequence url script-response-body https://raw.githubusercontent.com/kergb/QuantumultX/refs/heads/main/youtube.js

# QX Youtube 无中文字幕机翻方案//hostname= www.youtube.com
// 简体；订阅链接后面加上 #out=Hant 后开启资源解析器
// https://raw.githubusercontent.com/id77/QuantumultX/master/rewrite/Youtube_CC.conf#out=Hant

https:\/\/www.youtube.com\/api\/timedtext\?.+&lang=(?!(zh|zh\-Hans)&)((?!&tlang=zh\-Hans).)*$ url request-header \sHTTP/1\.1(\r\n) request-header &tlang=zh-Hans HTTP/1.1$1


// 繁体；订阅链接后面加上 #out=Hans 后开启资源解析器
// https://raw.githubusercontent.com/id77/QuantumultX/master/rewrite/Youtube_CC.conf#out=Hans

https:\/\/www.youtube.com\/api\/timedtext\?.+&lang=(?!zh\-Hant&)((?!&tlang=zh\-Hant).)*$ url request-header \sHTTP/1\.1(\r\n) request-header &tlang=zh-Hant HTTP/1.1$1

hostname = rr*.googlevideo.com, youtubei.googleapis.com, www.youtube.com
   


